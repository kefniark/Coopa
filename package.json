{
	"name": "coopa",
	"version": "0.1.9",
	"description": "",
	"main": "build/coopa.umd.js",
	"module": "build/coopa.es.mjs",
	"types": "build/declarations/index.d.ts",
	"sideEffects": false,
	"engines": {
		"node": ">=12.0.0",
		"npm": ">=6.0.0"
	},
	"author": {
		"name": "Kevin destrem",
		"email": "kef1@free.fr"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git@github.com:kefniark/Coopa.git"
	},
	"bugs": {
		"url": "https://github.com/kefniark/Coopa/issues"
	},
	"scripts": {
		"build": "run-s build:**",
		"build:esm": "rollup -c rollup.config.js --file build/coopa.es.mjs --format esm",
		"build:umd": "rollup -c rollup.config.js --file build/coopa.umd.js --format umd --name \"Coopa\"",
		"build:tsc": "tsc --emitDeclarationOnly --declaration true",
		"coverage": "jest --coverage",
		"dev": "rollup -c rollup.dev.js --watch",
		"dev:livereload": "livereload samples/",
		"prettier": "run-s prettier:**",
		"prettier:src": "prettier --config .prettierrc.yaml --write \"src/**/*.ts\"",
		"prettier:tests": "prettier --config .prettierrc.yaml --write \"tests/**/*.ts\"",
		"test": "run-s test:**",
		"test:unittest": "jest",
		"test:lint": "prettier --config .prettierrc.yaml --check \"src/**/*.ts\"",
		"test:depencencies": "npm-check --production || echo \"Run 'npm run update' to interactively update dependencies for this project\"",
		"update": "npm-check --update"
	},
	"devDependencies": {
		"@types/jest": "^24.0.23",
		"@types/jsdom": "^12.2.4",
		"@types/node": "^12.12.8",
		"fatina": "^3.0.1",
		"jest": "^24.9.0",
		"jsdom": "^15.2.1",
		"livereload": "^0.8.2",
		"npm-check": "^5.9.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.19.1",
		"rollup": "^1.27.0",
		"rollup-plugin-filesize": "^6.2.1",
		"rollup-plugin-serve": "^1.0.1",
		"rollup-plugin-typescript2": "^0.25.2",
		"rollup-plugin-version-injector": "^1.1.3",
		"ts-jest": "^24.1.0",
		"ts-loader": "^6.2.1",
		"tslib": "^1.10.0",
		"typescript": "^3.7.2"
	}
}
